web: bash -lc 'set -e; chmod 755 ./.qgtunnel/bin/qgtunnel; export QGTUNNEL_CONFIG=./qgtunnel.yml; echo "=== BOOT: printing QGTUNNEL_URL and qgtunnel.yml ==="; echo "QGTUNNEL_URL=${QGTUNNEL_URL}"; echo "----- qgtunnel.yml (cat -A) -----"; cat -A ./qgtunnel.yml; echo "----- end -----"; ./.qgtunnel/bin/qgtunnel & for i in {1..40}; do (echo > /dev/tcp/127.0.0.1/2222) >/dev/null 2>&1 && echo "[BOOT] qgtunnel listening on 127.0.0.1:2222" && break; sleep 0.5; done; N8N_HOST=0.0.0.0 N8N_PORT=$PORT n8n start'